---
const PUBLIC_GA_TRACKING_ID = import.meta.env.PUBLIC_GA_TRACKING_ID;
---

<script type="text/partytown" src={`https://www.googletagmanager.com/gtag/js?id=${PUBLIC_GA_TRACKING_ID}`}></script>
<script type="text/partytown" define:vars={{ PUBLIC_GA_TRACKING_ID }}>
window.dataLayer = window.dataLayer || [];
function gtag() {
    dataLayer.push(arguments);
}
gtag('js', new Date());
gtag('config', PUBLIC_GA_TRACKING_ID);
</script>
